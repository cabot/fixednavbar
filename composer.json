{
    "name": "cabot/fixednavbar",
    "type": "phpbb-extension",
    "description": "This extension fix the navbar on top when scrolling down.",
    "homepage": "http://cabotweb.fr",
    "version": "1.0.1",
    "keywords": [
        "phpbb",
        "extension",
        "Fixed navbar"
    ],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Vjacheslav Trushkin",
            "homepage": "https://iconify.design",
            "role": "Developer"
        },
        {
            "name": "cabot",
            "homepage": "https://cabotweb.fr",
            "role": "SushiMaker"
        }
    ],
    "require": {
        "php": ">=5.4",
        "composer/installers": "~1.0.0",
        "phpbb/phpbb": ">=3.1.0"
    },
    "extra": {
        "display-name": "Fixed navbar",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.0"
        },
		"version-check": {
			"host": "raw.githubusercontent.com",
			"directory": "/cabot/fixednavbar/master",
			"filename": "versioncheck.json",
            "ssl": true
		}
    }
}
